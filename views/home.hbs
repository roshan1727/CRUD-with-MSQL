<h3>Product Management System</h3>

<table>
    <thead>
        <tr>
            <td>#</td>
            <td>Product Name</td>
            <td>Price</td>
            <td>Location</td>
            <td>Edit</td>
            <td>Delete</td>
        </tr>
    </thead>
    <tbody>
       {{#each rows}}
       <tr>
        <td>{{this.ID}}</td>
        <td>{{this.NAME}}</td>
        <td>{{this.price}}</td>
        <td>{{this.location}}</td>
        <td></td>
        <td></td>
       </tr>
       {{/each}}
    </tbody>
</table>
